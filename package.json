{
  "name": "css-class-intellisense",
  "displayName": "CSS Class Intellisense",
  "version": "4.0.1",
  "description": "CSS Class Intellisense",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "icon": "images/icon.png",
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:vue",
    "onLanguage:css",
    "onLanguage:scss",
    "onLanguage:less"
  ],
  "main": "dist/node/main.js",
  "browser": "dist/browser/main.js",
  "scripts": {
    "watch": "tsc -w -p ./",
    "build:browser": "node scripts/browser.mjs",
    "build:node": "node scripts/node.mjs",
    "open-in-browser": "vscode-test-web --extensionDevelopmentPath=. .",
    "lint": "eslint src --ext ts",
    "publish": "vsce publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tarrowren/css-class-intellisense.git"
  },
  "keywords": [
    "html",
    "vue",
    "css",
    "scss",
    "less",
    "class",
    "intellisense"
  ],
  "author": "Tarrowren",
  "publisher": "tarrow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tarrowren/css-class-intellisense/issues"
  },
  "homepage": "https://github.com/Tarrowren/css-class-intellisense#readme",
  "devDependencies": {
    "@types/node": "^16.18.3",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-web": "^0.0.32",
    "eslint": "^8.28.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0"
  },
  "dependencies": {
    "@lezer/common": "^1.0.2",
    "@lezer/css": "^1.1.0",
    "@lezer/html": "^1.2.0",
    "@lezer/javascript": "^1.3.1"
  }
}
