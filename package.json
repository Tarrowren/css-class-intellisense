{
    "name": "css-class-intellisense",
    "displayName": "css-class-intellisense",
    "description": "css class 智能提示",
    "version": "3.0.0",
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:html"
    ],
    "main": "./client/out/extension",
    "contributes": {
        "configuration": {
            "title": "CSS Class Intellisense",
            "properties": {
                "cssClassIntellisense.remoteCSSCachePath": {
                    "type": "string",
                    "default": "",
                    "description": "If the field is empty, caching is not used."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -b",
        "watch": "tsc -b -w"
    },
    "devDependencies": {
        "@types/node": "^14.0.1",
        "@types/vscode": "^1.45.0",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "eslint": "^7.0.0",
        "typescript": "^3.8.3",
        "vscode-css-languageservice": "^4.1.2",
        "vscode-html-languageservice": "^3.0.3",
        "vscode-languageclient": "^6.1.3",
        "vscode-languageserver": "^6.1.1",
        "vscode-languageserver-textdocument": "^1.0.1"
    }
}
