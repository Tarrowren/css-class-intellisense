{
    "name": "css-class-intellisense",
    "displayName": "css class intellisense",
    "description": "css class 智能提示",
    "version": "2.0.0",
    "publisher": "Tarrow",
    "license": "MIT",
    "keywords": [
        "html",
        "css",
        "class",
        "intellisense"
    ],
    "engines": {
        "vscode": "^1.41.1"
    },
    "categories": [
        "Programming Languages",
        "Other"
    ],
    "icon": "./resources/icon.png",
    "activationEvents": [
        "onLanguage:html"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "configuration": {
            "title": "CSS Class Intellisense",
            "properties": {
                "Remote CSS Support": {
                    "type": "boolean",
                    "default": false,
                    "description": "Enable remote CSS support and create a \"remote_css\" folder automatically."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "webpack --mode production",
        "webpack": "webpack --mode development",
        "watch": "tsc -watch -p ./",
        "rebuild": "./node_modules/.bin/electron-rebuild.cmd"
    },
    "devDependencies": {
        "@types/node": "^13.1.4",
        "@types/request": "^2.48.4",
        "@types/vscode": "^1.41.0",
        "electron": "^6.1.5",
        "electron-rebuild": "^1.8.8",
        "request": "^2.88.0",
        "ts-loader": "^6.2.1",
        "tslint": "^5.20.1",
        "typescript": "^3.7.4",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10"
    },
    "bugs": {
        "url": "https://github.com/Tarrowren/css-class-intellisense/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Tarrowren/css-class-intellisense"
    },
    "homepage": "https://github.com/Tarrowren/css-class-intellisense/blob/master/README.md",
    "dependencies": {
        "tree-sitter": "^0.16.0",
        "tree-sitter-css": "^0.16.0",
        "tree-sitter-html": "^0.16.0"
    }
}
