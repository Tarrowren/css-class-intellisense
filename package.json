{
  "name": "css-class-intellisense",
  "displayName": "css-class-intellisense",
  "version": "3.2.0",
  "description": "CSS Class Intellisense",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:vue",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/client/node/main.js",
  "browser": "./dist/client/browser/main.js",
  "scripts": {
    "watch": "tsc -w -p ./",
    "package:browser": "node --experimental-import-meta-resolve ./scripts/browser.mjs",
    "open-in-browser": "vscode-test-web --extensionDevelopmentPath=. ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tarrowren/css-class-intellisense.git"
  },
  "keywords": [
    "html",
    "css",
    "class",
    "intellisense"
  ],
  "author": "Tarrowren",
  "publisher": "tarrow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tarrowren/css-class-intellisense/issues"
  },
  "homepage": "https://github.com/Tarrowren/css-class-intellisense#readme",
  "devDependencies": {
    "@types/node": "^16.11.60",
    "@types/node-fetch": "^2.6.2",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vscode/test-web": "^0.0.30",
    "eslint": "^8.24.0",
    "path-browserify": "^1.0.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0"
  },
  "dependencies": {
    "@lezer/common": "^1.0.1",
    "@lezer/css": "^1.0.0",
    "@lezer/html": "^1.0.1",
    "ajv": "^8.11.0",
    "node-fetch": "^2.6.7",
    "request-light": "^0.5.8",
    "vscode-languageclient": "^8.0.2",
    "vscode-languageserver": "^8.0.2",
    "vscode-languageserver-textdocument": "^1.0.7",
    "vscode-uri": "^3.0.6"
  }
}
