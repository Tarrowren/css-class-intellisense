{
	"name": "css-class-intellisense-alpha",
	"displayName": "css class intellisense",
	"description": "css class 智能提示",
	"version": "1.0.1",
	"publisher": "Tarrow",
	"license": "MIT",
	"keywords": [
		"html",
		"css",
		"class",
		"intellisense"
	],
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Programming Languages",
		"Other"
	],
	"icon": "image/icon.png",
	"activationEvents": [
		"onLanguage:html"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"configuration": {
			"title": "CSS Class Intellisense",
			"properties": {
				"Remote CSS Support": {
					"type": "boolean",
					"default": false,
					"description": "Enable remote CSS support and create a \"remote_css\" folder automatically."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.12.17",
		"@types/request": "^2.48.3",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"request": "^2.88.0",
		"ts-loader": "^6.2.1",
		"tslint": "^5.20.0",
		"typescript": "^3.7.3",
		"vscode-css-languageservice": "^4.0.2",
		"vscode-html-languageservice": "^3.0.3",
		"vscode-test": "^1.3.0",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10"
	},
	"bugs": {
		"url": "https://github.com/Tarrowren/css-class-intellisense-alpha/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Tarrowren/css-class-intellisense-alpha"
	},
	"homepage": "https://github.com/Tarrowren/css-class-intellisense-alpha/blob/master/README.md"
}